--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/missing_or_invalid_mesh_msg.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int numvariations;
string filepath;
string deadfilepath;
string meshpath;
string meshdeadpath;
string newpath;
string newdeadpath;
string reftypevariant;
string reftypedeadvariant;
float deadratio;

if(isvalidindex(d@plant_definition, \"variants\"))
{
    numvariations = d@plant_definition[\"variants\"];
}

for (int i = 1; i <= numvariations; i++)
{
    
    if(d@plant_definition[\"reftype\" + itoa(i)] == 1)
    {
        reftypevariant = \"meshvariant\";
        reftypedeadvariant = \"meshdeadvariant\";
    }
    else
    {
        reftypevariant = \"filemeshvariant\";
        reftypedeadvariant = \"filemeshdeadvariant\";
    }
    
    meshpath = d@plant_definition[reftypevariant + itoa(i)];
    meshdeadpath = d@plant_definition[reftypedeadvariant + itoa(i)];
    deadratio = d@plant_definition[\"deadratio\" + itoa(i)];
    
    if(startswith(meshpath, \"../\") || startswith(meshpath, \"/obj\") && reftypevariant == \"filemeshvariant\")
    {
        warning(\"Mesh Variant \" + itoa(i) + \" is an operator path. Switch 'Mesh Variant from' to 'Operator Path'.\");
    }
    
    newpath = opfullpath(meshpath);
    if(startswith(newpath,\"/obj\"))
    {
        newpath = \"op:\" + opfullpath(newpath);   
    }
    if(npoints(newpath) == 0)
    {
        warning(\"Invalid SOP or file specified for Mesh Variant \" + itoa(i));
    }
    
    newdeadpath = opfullpath(meshdeadpath);
    if(startswith(newdeadpath,\"/obj\"))
    {
        newdeadpath = \"op:\" + opfullpath(newdeadpath);  
    }
    if(npoints(newdeadpath) == 0 && deadratio > 0)
    {
        warning(\"Invalid SOP or file specified for Mesh (Dead) Variant \" + itoa(i));
    }
      
}
"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
