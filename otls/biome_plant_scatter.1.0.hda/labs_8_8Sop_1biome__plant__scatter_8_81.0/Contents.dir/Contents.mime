{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"
//  In this version of plant scatter, plants with 
//  a greater spacing radius are given priority.
//  Points within the spacing of a larger spacing tree
//  are removed from the scatter, UNLESS that point is
//  a shrub type. Shrubs require less spacing to grow
//  and are assumed to be shade tolerant in this setup. 

int limit = 1;
int pts[] = nearpoints(0, @P, @_bps_radius);
if (len(pts) > limit) 
{
   foreach(int pt; pts) 
   {
        if(@_bps_radius < point(0, \"_bps_radius\", pt))
        {
            if(@ptnum < pt)
            {
                removepoint(0, @ptnum);
            }
        }
        else if(@_bps_radius == point(0, \"_bps_radius\", pt))
        {
            if(@ptnum < pt)
            {
                removepoint(0, @ptnum);
            }
        }
        else if(point(0,\"_bps_type\", pt) == 0) //if the smaller spacing radius is a tree type
        {
            if(@ptnum < pt)
            {
                setpointgroup(0, \"_bps_colliding\", pt, 1, \"set\");
            }
        }     
   }
}"	)
