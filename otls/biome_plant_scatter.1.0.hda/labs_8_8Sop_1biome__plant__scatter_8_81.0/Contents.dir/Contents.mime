--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/assign_mesh_based_on_age_and_ifalive.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"//find a mesh variant to match the current point's age
int numvariations;
float minage;
float maxage;
float minages[];
float maxages[];
i[]@_bps_possiblevariations;
f[]@_bps_weights;
int variation;
s@_bps_mesh_path;
s@_bps_unreal_path;
float minscale;
float maxscale;
int scaleby;
float weight;
float difference;
float diff_min;
int closest_index;
int enginepaths = 0;
@group__bps_incompleteagecoverage = 0;

if(isvalidindex(d@plant_definition, \"enginepaths\"))
{
    enginepaths = d@plant_definition[\"enginepaths\"];
}
if(isvalidindex(d@plant_definition, \"variants\"))
{
    numvariations = d@plant_definition[\"variants\"];
}

for (int i = 1; i <= numvariations; i++)
{
    
    if(isvalidindex(d@plant_definition, \"agemin\" + itoa(i)))
    {
        minage = d@plant_definition[\"agemin\" + itoa(i)];
        append(minages, minage);
    }
    
    if(isvalidindex(d@plant_definition, \"agemax\" + itoa(i)))
    {
        maxage = d@plant_definition[\"agemax\" + itoa(i)];
        append(maxages, maxage);
    }
    
    //if my current age is within this variations range, store the possible variation and the weight
    //else get the distance to the min and max, keep the min distance and the variation
    if(@_bps_age >= minage && @_bps_age <= maxage)
    {
        append(@_bps_possiblevariations,i);
        if(isvalidindex(d@plant_definition, \"weight\" + itoa(i)))
        {
            //get list of corresponding weights
            weight = d@plant_definition[\"weight\" + itoa(i)];
            append(@_bps_weights, weight);
        }
    }  
}

//choose variation
if(len(@_bps_possiblevariations) == 0)
{
    @group__bps_incompleteagecoverage = 1;
    if(chi('missingmeshvariation') == 2)
    {
        @group__bps_incompleteagecoverage = 0;
        diff_min = 1000000.0;
        difference = 10000000.0;
        closest_index = -1;
        foreach(int i; float val; minages)
        {
            difference = abs(val - @_bps_age);
            if(difference < diff_min)
            {
                diff_min = difference;
                closest_index = i;
            }
        }
    
        foreach(int i; float val; maxages)
        {
            difference = abs(val - @_bps_age);
            if(difference < diff_min)
            {
                diff_min = difference;
                closest_index = i;
            }
        }
        variation = closest_index+1;
    }
    
}
else if(len(@_bps_possiblevariations)==1)
{
    variation = @_bps_possiblevariations[0];
}
else
{
    int weightedrandindex = sample_discrete(@_bps_weights, rand(@ptnum + 20 * 30));
    variation = @_bps_possiblevariations[weightedrandindex];
}

//choose mesh path and unreal path based on if dead or not
if(isvalidindex(d@plant_definition, \"deadratio\" + itoa(variation)))
{
    if(rand(@ptnum*23+74) <= d@plant_definition[\"deadratio\" + itoa(variation)])
    {
        if(isvalidindex(d@plant_definition, \"meshdeadvariant\" + itoa(variation)))
        {
            s@_bps_mesh_path = d@plant_definition[\"meshdeadvariant\" + itoa(variation)];    
        }
        if(enginepaths)
        {
            if(isvalidindex(d@plant_definition, \"unrealassetdeadpath\" + itoa(variation)))
            {
                s@_bps_unreal_path = d@plant_definition[\"unrealassetdeadpath\" + itoa(variation)];    
            }
        }
    }
    else
    {
        if(isvalidindex(d@plant_definition, \"meshvariant\" + itoa(variation)))
        {
            s@_bps_mesh_path = d@plant_definition[\"meshvariant\" + itoa(variation)];    
        }
        if(enginepaths)
        {
            if(isvalidindex(d@plant_definition, \"unrealassetpath\" + itoa(variation)))
            {
                s@_bps_unreal_path = d@plant_definition[\"unrealassetpath\" + itoa(variation)];    
            }
        }
    }      
}
// if this is object-level geometry to be instanced, 
// prepend op: to the full path so the instance node 
// can differentiate between a file path and a node path
if(startswith(opfullpath(s@_bps_mesh_path), \"/obj\"))
{
    s@_bps_mesh_path = \"op:\" + opfullpath(s@_bps_mesh_path);
}

//set pscale based on scaleby type (0=random, 1=age) between variations min and max scale
if(isvalidindex(d@plant_definition, \"scalemin\" + itoa(variation)))
{
    minscale = d@plant_definition[\"scalemin\" + itoa(variation)];
}
if(isvalidindex(d@plant_definition, \"scalemax\" + itoa(variation)))
{
    maxscale = d@plant_definition[\"scalemax\" + itoa(variation)];
}
if(isvalidindex(d@plant_definition, \"randscale\"))
{
    scaleby = d@plant_definition[\"randscale\"];
}


//if random scale method, else scale by age
if(scaleby == 1)
{
    @pscale = fit01(rand(@ptnum * 30 + 12), minscale, maxscale);
}
else
{
    @pscale = fit01(@_bps_age, minscale, maxscale);
}

if(enginepaths == 1)
{
    s@unreal_instance = s@_bps_unreal_path;
}

@scale = {1, 1, 1};
"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
vex_precision	[ 0	locks=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
missingmeshvariation	[ 8	locks=0 ]	(	[ missingmeshvariation	0 ] 	)
}

