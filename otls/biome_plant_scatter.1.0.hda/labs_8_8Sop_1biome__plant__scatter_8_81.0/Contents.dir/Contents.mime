--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/measure_viability.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
bindeach	[ 0	locks=0 ]	(	"off"	)
snippet	[ 0	locks=0 ]	(	"
//  Given the species' preference for temperature
//  and precipitation, along with the temperature 
//  and precipitation of the environment,
//  Measure the viability of the current plant species
//  at every voxel of the environment. 

dict dictval = point(-1, \"plant_definition\", 0);
float tl; //temperature lower limit
float tu; //temperature upper limit
float tp; //temperature preference, plant's ideal temperature

float pl; //precipitation lower limit
float pu; //precipitation upper limit
float pp; //precipitation preference

float h; //hardiness, increasing hardiness increases the range for maximum viability

if(isvalidindex(dictval, \"templower\"))
{
    tl = dictval[\"templower\"];
}
if(isvalidindex(dictval, \"tempupper\"))
{
    tu = dictval[\"tempupper\"];
}
if(isvalidindex(dictval, \"temppref\"))
{
    tp = dictval[\"temppref\"];
}

if(isvalidindex(dictval, \"preclower\"))
{
    pl = dictval[\"preclower\"];
}
if(isvalidindex(dictval, \"precupper\"))
{
    pu = dictval[\"precupper\"];
}
if(isvalidindex(dictval, \"precpref\"))
{
    pp = dictval[\"precpref\"];
}

if(isvalidindex(dictval, \"hardiness\"))
{
    h = dictval[\"hardiness\"];
}

h = clamp(h, .001, .999);


float measureadaptability(float u; float l; float p; float h; float c)
{
    float stddevu = (u - p) / 3.0;
    float stddevl = (p - l) / 3.0;
    float ef = 2.71828;
    float a;
    float stddev;
    
    if(c>p)
    {
        stddev = stddevu;
    }
    else
    {
        stddev = stddevl;
    }
    
    a = pow(ef, ((-pow((c - p), 2)) / (2 * pow(stddev, 2))));
    return (1 - pow(1 - h, a)) / h;
}

float ta = measureadaptability(tu, tl, tp, h, @_bps_temperature);
float pa = measureadaptability(pu, pl, pp, h, @_bps_precipitation);

float viability = pa * ta * @_bps_soil;

@mask = viability;
@Cd = point(1, \"Cd\", 0);
"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
vex_geometrygenerator	[ 0	locks=0 ]	(	"off"	)
vdb_signedflood	[ 0	locks=0 ]	(	"on"	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_precision	[ 0	locks=0 ]	(	auto	)
folder1	[ 0	locks=0 ]	(	0	0	)
spare_input0	[ 0	locks=0 ]	(	../PLANT_TYPE1	)
}

