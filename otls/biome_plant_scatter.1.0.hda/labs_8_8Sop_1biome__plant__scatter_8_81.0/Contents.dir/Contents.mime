int limit = 1;
float dist = 10000000000;
int pts[] = nearpoints(0, @P, @_bps_maxradius*2);
if (len(pts) > limit) 
{
   foreach(int pt; pts) 
   {
        if(pt != pts[0])//if its not self
        {
            dist = length(@P - point(0, \"P\", pt));
            if(dist < (@_bps_radius + point(0, \"_bps_trunkradius\", pt)))
            {
                if(@_bps_type > point(0, \"_bps_type\", pt))//if the current ptnum is a shrub and the nearpoint is a tree, remove current ptnum 
