snippet	[ 0	locks=0 ]	(	"// Computes orient
if (chi(\"rotation_mode\") == 0)
    p@out_orient = p@_pex_orient;
else
    p@out_orient = quaternion(maketransform(normalize(v@_pex_N), normalize(v@_pex_up)));

// Ensures the w component of the quaternion is never negative
// because it will be discarded during export and derived in the game engine
if (p@out_orient.w < 0)
    p@out_orient *= -1.0;

if (chi(\"has_scale\") == 0)
    v@_pex_scale = {1,1,1};

if (chi(\"has_pscale\") == 0)
    f@_pex_pscale = 1;
    
v@out_scale = v@_pex_scale;
f@out_pscale = f@_pex_pscale;
// Multiplies scale by uniform scale because only scale will be exported
v@out_scale = v@_pex_scale * f@_pex_pscale;


// Ensures the mesh, material and custom attributes have default values even if they do not exist
f@out_mesh = i@_pex_mesh;
f@out_mat = i@_pex_mat;
f@out_mat_1 = i@_pex_mat_1;
f@out_mat_2 = i@_pex_mat_2;
